#include <stdio.h>

int main() {
  int x,flag=0,n=2;
  
  printf("Enter x: ");
  scanf("%d",&x);
  
  if(x==1)
     printf("1 is neither prime nor composite");
  else if(x==2)
     printf("2 is prime");
  else
  {
    while(x>=n*n)
     {
       if(x%n==0)
         flag=1;
       n++;
      }
  
   if(flag==1)
     printf("%d is not prime",x);
   else
     printf("%d is prime",x);
  }  
  return 0;
}
